{
  "version": "0.0.1",
  "openApi": {
    "enabled": true,
    "title": "OpenAPI Title",
    "security": {
      "apiKey": {
        "name": "Api-Key",
        "in": "header"
      },
      "http": {
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "files": {
    "include": [
      "working-examples/complex-api/*.js"
    ]
  },
  "format": "json",
  "outDir": "./api",
  "srcDir": "./docs"
}